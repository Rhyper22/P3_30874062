<!DOCTYPE html>
<html lang="es">
<head>

  <meta charset="UTF-8">
  <title>categoriass</title>
  <link rel="stylesheet" type="text/css" href="/css/producto.css">
  
</head>
<body>

  <div id='botonera'>
    
    <a href="/logout2" class='botonProductos'>Cerrar</a>
    <a href="/usuarios" class='botonProductos'>Users</a>
    <a href="/compras" class='botonProductos'>Compras</a>
    <a href="/productos" class='botonProductos'>Productos</a>
    <a href='/categorias' class='botonProductos'>Categorias</a>

  </div>

  <h1>Categorias</h1>
  <div class="table">
    <table>
          <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th><a href="/addCategorias"><img src="/img/add.png" id='add'></a></th>
          </tr>
          <%for(const categorias of modelo){%>
            <tr>
              <td><%=categorias.id%></td>
              <td><%=categorias.nombre%></td>
              <td class="icon">

              <a href="/updateCategoria/<%=categorias.id%>"><img src="/img/update.png" width="30"></a>
              <a href="/eliminarCategoria/<%=categorias.id%>"><img src="img/delete.png" width="30"></a>

             </td>
            </tr>
           <%}%>
        </table>
       
  </div>
</body>
</html>
