    <!DOCTYPE html>
    <html>
      <head>
        <title>Login_Usuario</title>
        <link rel='stylesheet' href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css
        ' />
        <link rel="stylesheet" href="style.css">
        <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
      </head>
      <body>
          <table class="table table-bordered table-striped text-center mt-4">
            <a href="/insert" class="button"><i class='bx bxs-plus-circle bx-md'></i></a>
            <thead>
              <tr>
                <th scope="col">ID</th>
                <th scope="col">Nombre</th>
                <th scope="col">CÃ³digo</th>
                <th scope="col">Precio</th>
                <th scope="col">Descripcion</th>
                <th scope="col">FCardiaca</th>
                <th scope="col">DRecorrida</th>
                <th scope="col">Correo</th>
                <th scope="col">Categoria</th>
                <th scope="col">Accion</th>
                <th scope="col">Accion</th>

              </tr>
            </thead>
            <tbody>
              <% productos.forEach(Producto=> { %>
              <tr>
                <td><%=producto.id%></td>
                <td><%=producto.Nombre%></td>
                <td><%=producto.Codigo%></td>
                <td><%=producto.Precio%></td>
                <td><%=producto.Descripcion%></td>
                <td><%=producto.FCardiaca%></td>
                <td><%=producto.DRecorrida%></td>
                <td><%=producto.Correo%></td>
                <td><% if (producto.categoria_id == 1) { %></td>
                  Sony
                <% } else if (producto.categoria_id == 2) { %></td>
                  Samsung
                <% } else if (producto.categoria_id == 3) { %></td>
                  Apple
                <% } else if (producto.categoria_id == 4) { %></td>
                  LG
                <% } else if (producto.categoria_id == 5) { %></td>
                  Samsung Gear 2
                <% } else if (producto.categoria_id == 6) { %></td>
                  Motorola
                <% } else if (producto.categoria_id == 7) { %></td>
                  Huawei
                <% } %>
                
                <td><a class="btn btn-primary" href="/edit/<%= user.ID %>"></a>Editar</td>
                <td><a class="btn btn-danger" href="/delete/<%= user.ID %>"></a>Borrar</td>
              </tr>
              <% }); %>
          </table>
      </body>
    </html>

       